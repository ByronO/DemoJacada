<Window x:Class="DemoJacada.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DemoJacada"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Margin="10,42,0,0">
      
        <Border Grid.ColumnSpan="3">
            <Grid>
                <Button x:Name="abrir" Content="abrir" HorizontalAlignment="Left" Margin="204,0,0,335" Click="abrir_Click" Height="19" VerticalAlignment="Bottom" Width="108"/>
                <Button x:Name="cerrar" Content="cerrar" HorizontalAlignment="Left" Margin="323,0,0,335" Click="cerrar_Click" Height="19" VerticalAlignment="Bottom" Width="108"/>
                <TextBox x:Name="txtUrl"  HorizontalAlignment="Left" Margin="10,0,0,335"  Height="19" VerticalAlignment="Bottom" Width="183"/>
            </Grid>
        </Border>
        <Border Margin="-10,17,0,0"  Grid.ColumnSpan="3">
            <Grid Margin="-10,69,0,0" x:Name="JacadaGrid" Grid.ColumnSpan="2">

            </Grid>
        </Border>
    </Grid>
</Window>
