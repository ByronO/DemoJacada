<Window x:Class="DemoJacada.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DemoJacada"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Margin="10,42,0,0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="38*"/>
            <ColumnDefinition Width="353*"/>
        </Grid.ColumnDefinitions>
        <Border Grid.ColumnSpan="2">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="31*"/>
                    <ColumnDefinition Width="230*"/>
                </Grid.ColumnDefinitions>
                <Button x:Name="abrir" Content="abrir" HorizontalAlignment="Left" Margin="64,-30,0,388" Click="abrir_Click" Height="19" VerticalAlignment="Bottom" Width="108" Grid.Column="1"/>
                <Button x:Name="cerrar" Content="cerrar" HorizontalAlignment="Left" Margin="213,-30,0,388" Click="cerrar_Click" Height="19" VerticalAlignment="Bottom" Width="108" Grid.Column="1"/>

            </Grid>
        </Border>
        <Border Margin="-10,0,0,0"  Grid.ColumnSpan="2">
            <WebBrowser  Name="JacadaBrowser" Margin="122,38,30,10" />

        </Border>
    </Grid>
</Window>
