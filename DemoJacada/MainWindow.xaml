<Window x:Class="DemoJacada.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DemoJacada"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Margin="10,42,0,0">
        <Border Grid.Row="3" Grid.RowSpan="2" Grid.Column="1">
            <Grid>
                <Button x:Name="btnZoom" Content="Call" HorizontalAlignment="Left" Margin="5,-30,0,388" Click="btnZoom_Click" Height="19" VerticalAlignment="Bottom" Width="108"/>

            </Grid>
        </Border>
        <Border Margin="-10,0,0,0">
            <WebBrowser Name="JacadaBrowser" />

        </Border>
    </Grid>
</Window>
